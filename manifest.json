{
  "name": "Stay",
  "description":
    "Restore browser location and position on display resolution changes.",
  "version": "0.1",
  "permissions": ["storage"],
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "options_page": "options.html",
  "manifest_version": 2,
  "minimum_chrome_version": "17.0"
}
